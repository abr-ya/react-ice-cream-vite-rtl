{
  "name": "react-vite-eslint-test",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "node ./node_modules/eslint/bin/eslint.js --ext .jsx --ext .js --ext .ts --ext .tsx src/",
    "fix": "prettier-eslint --write 'src/**/*.{js,jsx}'",
    "test": "jest -env=jsdom --config=jest-config.json --watch"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-testing-library": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.6",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.6",
    "jest-circus": "^26.5.6",
    "jest-scss-transform": "^1.0.1",
    "jest-watch-typeahead": "^0.6.4",
    "msw": "^0.35.0",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "sass": "^1.36.0",
    "vite": "^2.4.4"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "babel-plugin-transform-vite-meta-env"
        ],
        "presets": [
          "react-app"
        ]
      }
    }
  }
}
